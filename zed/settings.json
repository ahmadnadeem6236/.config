{
  "edit_predictions": {
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "project_panel": {
    "dock": "right"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  },
  "ui_font_family": "Monaspace Neon",
  "buffer_font_family": "Monaspace Neon",
  "features": {
    "edit_prediction_provider": "zed",
    "copilot": false
  },
  "hide_mouse": "on_typing_and_movement",
  // Will be triggered manually with `ctrl-x ctrl-c`.
  "show_edit_predictions": false,
  "inlay_hints": {
    "enabled": false
  },
  "theme": "Catppuccin Mocha (Blur)",
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 18,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "tab_bar": {
    "show": true
  },
  "scrollbar": {
    "show": "never"
  },
  // Rainbow indentation
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware"
  },
  "preview_tabs": {
    "enabled": false
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "vim": {
    "use_system_clipboard": "never"
  },
  "languages": {
    "TypeScript": {
      "show_edit_predictions": true
    },
    "LaTeX": {
      "soft_wrap": "preferred_line_length"
    }
  },
  "wrap_guides": [90],
  "show_wrap_guides": true
